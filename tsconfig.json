{
    "compilerOptions": {
      "esModuleInterop": true,
      "module": "CommonJS",
      "jsx": "react",
      "target": "ES2022",
      "moduleResolution": "Node",
      "strictNullChecks": true,
      "noImplicitAny": true,
      "strict": true,
      "isolatedModules": true,
      "noEmit": true,
      "outDir": "./dist"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  